{
  "name": "@leinad54/docker-ts",
  "type": "module",
  "version": "0.0.8",
  "packageManager": "pnpm@9.12.2",
  "description": "",
  "license": "MIT",
  "repository": "daniel-le97/node-docker",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "generate": "openapi --input ./src/open-api.yaml --output ./src/generated --client axios",
    "dev": "vitest dev",
    "play": "jiti playground",
    "lint": "eslint . --fix ",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "prepack": "pnpm run build",
    "release": "pnpm test && changelogen --release && npm publish --access public && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "axios": "^1.6.1",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.0",
    "@types/node": "^20.9.0",
    "@vitest/coverage-v8": "^0.34.6",
    "changelogen": "^0.5.5",
    "jiti": "^1.21.0",
    "openapi-typescript-codegen": "^0.25.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  }
}